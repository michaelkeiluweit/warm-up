{
    "name": "michaelkeiluweit/warm-up",
    "description": "Warms up the OXID eShop.",
    "type": "oxideshop-component",
    "autoload": {
        "psr-4": {
            "MichaelKeiluweit\\WarmUp\\": "src/",
            "MichaelKeiluweit\\WarmUp\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "authors": [
        {
            "name": "Michael Keiluweit",
            "email": "michael.keiluweit@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "conflict": {
        "oxid-esales/oxideshop-ce": "<7.0"
    },
    "require": {
        "php": ">=8.1",
        "mikey179/vfsstream": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "doctrine/dbal": "^2",
        "oxid-esales/oxideshop-ce": "dev-b-7.1.x",
        "curl/curl": "^2"
    },
    "config": {
        "allow-plugins": {
            "oxid-esales/oxideshop-unified-namespace-generator": true,
            "oxid-esales/oxideshop-composer-plugin": true
        }
    }
}
